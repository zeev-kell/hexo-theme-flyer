<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="google-site-verification" content="zeev-kell-xxx" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="keyword"  content="<%= page.keyword || config.keyword %>">
    <meta name="description" content="<%= page.subtitle || config.description %>">
    <link rel="shortcut icon" href="<%= config.root %>img/favicon.ico">
    <title>
        <% if (page.title) { %>
          <%= page.title + ' - ' + config.SEOTitle %>
        <% } else{%>
          <%= config.SEOTitle %>
        <% } %>
    </title>

    <link rel="canonical" href="<%= config.url + config.root + (page.path||'').replace('index.html', '') %>">
    
    <%- css('css/common.min') %>

</head>
