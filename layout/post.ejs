---
layout: layout
---

<!-- Post  -->
<article>
    <div class="container">
        <div class="row">
            <div class="col-md-9 post-container">
                <%- page.content %>
                <ul class="pager">
                    <% if (page.prev){ %>
                    <li class="previous">
                        <a href="<%- config.root %> <%- page.prev.path %>" title="<%- page.prev.title %>">
                            <%- page.prev.title %>
                        </a>
                    </li>
                    <% } %>
                    <% if (page.next){ %>
                    <li class="next">
                        <a href="<%- config.root %> <%- page.next.path %>" title="<%- page.next.title %>">
                            <%- page.next.title %>
                        </a>
                    </li>
                    <% } %>
                </ul>

            </div>
            <%- partial('_partial/post-content') %>
        </div>
    </div>
</article>

<% if(config['anchorjs']) { %>
<%- partial('_partial/anchor') %>
<% } %>
