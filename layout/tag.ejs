---
layout: page
---

<!-- tag -->
<section>
	<section>
	    <div class="tags">
	        <% site.tags.map(function(tag){  %>
	            <% if(tag.length > config['tag-condition-size']) { %>
	            	<a class="tag <% if(tag.name == page.tag){ %><%= 'active' %><% } %>" href="<%= config.root + 'tags/' + tag.name %>/" title="<%= tag.name %>">
					  <%= tag.name %>
					</a>
	            <% } %>
	        <% }) %>
	    </div>
	</section>
	<%- partial('_partial/archive', { type: 'tag' }) %>
</section>
